class Solution {
public:
    bool rotateString(string s, string goal) {
        int len = s.length();
        if(s == goal) return true;
        int count = 0;
        while(count < len - 1){
            char ch = s[0];
            for(int i = 1; i < len; i++) s[i-1] = s[i];
            s[len - 1] = ch;
            if(s == goal) return true;
            ++count;
        }
        return false;
    }
};


Time Complexity: O(n^2)
Space Complexity: O(1)
