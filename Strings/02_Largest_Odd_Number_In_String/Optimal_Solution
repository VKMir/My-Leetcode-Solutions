class Solution {
public:
    string largestOddNumber(string num) {
        int n = num.size();
        string ans = "";
        for(int j = n - 1; j >= 0; j--){
            int n = num[j] -'0';
            if(n % 2 != 0){
                ans += num.substr(0, j + 1);
                break;
            }
        }
        return ans;
    }
};


Time Complexity: O(n)
Space Complexity: O(1)
