class Solution {
public:
    bool isAnagram(string s, string t) {
        int m = s.length();
        int n = t.length();
        if(m != n) return false;
        sort(s.begin(), s.end());
        sort(t.begin(), t.end());
        if(s != t) return false;
        return true;
    }
};


Time Complexity: O(n log n)
Space Complexity: O(1)
